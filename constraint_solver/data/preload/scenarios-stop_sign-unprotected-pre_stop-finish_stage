or(
  and(
    GetOverlapOnReferenceLine,
    <=(
      +(
        apollo::planning::ReferenceLineInfo.AdcSlBoundary.end_s,
        *(
          -(
            1.0
          ),
          struct.apollo::hdmap::PathOverlap.start_s
        )
      ),
      0.3
    ),
    <=(
      Instance.linear_velocity,
      GetConfig.vehicle_param.max_abs_speed_when_stopped
    ),
    <=(
      +(
        struct.apollo::hdmap::PathOverlap.start_s18,
        *(
          -(
            1.0
          ),
          apollo::planning::ReferenceLineInfo.AdcSlBoundary.end_s
        )
      ),
      apollo::planning::scenario::stop_sign::StopSignUnprotectedStageCreep.scenario_config_.max_valid_stop_distance
    )
  ),
  and(
    GetOverlapOnReferenceLine,
    not(
      <=(
        +(
          apollo::planning::ReferenceLineInfo.AdcSlBoundary.end_s,
          *(
            -(
              1.0
            ),
            struct.apollo::hdmap::PathOverlap.start_s
          )
        ),
        0.3
      )
    )
  )
)