        )
      ),
      apollo::planning::scenario::traffic_light::TrafficLightUnprotectedRightTurnStageCreep.scenario_config_.min_pass_s_distance
    ),
    apollo::planning::scenario::traffic_light::TrafficLightUnprotectedRightTurnStageCreep.scenario_config_.enable_right_turn_on_red,
    not(
      ==(
        apollo::planning::scenario::traffic_light::TrafficLightUnprotectedRightTurnStageCreep.GetContext.stop_start_time,
        0.0
      )
    ),
    not(
      <=(
        +(
          NowInSeconds,
          *(
            -(
              1.0
            ),
            apollo::planning::scenario::traffic_light::TrafficLightUnprotectedRightTurnStageCreep.GetContext.stop_start_time65
          )
