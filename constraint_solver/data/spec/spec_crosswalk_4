and(
    cross(
        vehicle-ego.trajectory,
        crosswalk.boundary
    ),
    cross(
        pedestrian.boundary,
        road_boundary
    ),
    cross(
        pedestrian.boundary,
        crosswalk.boundary
    ),
    cross(
        crosswalk.boundary,
        intersection.boundary
    ),
    <(
        vehicle-ego.end_s,
        intersection.start_s
    )
)