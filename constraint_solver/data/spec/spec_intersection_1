and(
    not(
        overlap_finished
    ),
    is_approaching(
        vehicle-ego,
        intersection.start_s
    ),
    is_direction(
        vehicle-ego.lane,
        left
    ),
    is_color(
        traffic_light-self,
        unknown
    ),
    is_color(
        traffic_light-main,
        green
    ),
    not(
        is_yield(
            vehicle-ego,
            vehicle,
            intersection.start_s,
            intersection.end_s
        )
    )
)